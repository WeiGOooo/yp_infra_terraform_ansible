[Unit]
Description=Manage sausage-store-frontend.service


[Service]
AmbientCapabilities=CAP_NET_BIND_SERVICE
ExecStart=http-server {{ frontend_path }}/frontend -p 80 --proxy http://localhost:8080
StandardOutput=append:{{ frontend_path }}/logs/out-front.log
StandardError=append:{{ frontend_path }}/logs/out-front.log
User={{ frontend_user }}
Restart=always

[Install]
WantedBy=multi-user.target